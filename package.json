{
  "name": "Dolo",
  "version": "1.0.0",
  "description": "Donation Location App",
  "main": "server.js",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "method-override": "^2.3.10",
    "mysql2": "^1.5.2",
    "prettier": "^1.13.5",
    "sequelize": "^4.37.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "server": "node server.js",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "client": "node scripts/start-client.js",
    "build": "node scripts/build.js"
  }
}
